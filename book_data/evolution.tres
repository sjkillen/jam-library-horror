[sub_resource type="Resource" id="Resource_vjrc7"]
script = ExtResource("1_solq6")
title = "(A journal with many ripped out pages)"
body = "After seeing the growth myself it resembles a blobfish brought out of the ocean floor. But as I have been studying the dissected parts I've noticed signs that the tissue is still mutating and growing. I will keep a section to study and test even after the majority of it is removed, who knows this could be a breakthrough in evolutionary biology as we know it. 
"
