[sub_resource type="Resource" id="Resource_8nje7"]
script = ExtResource("1_tlfoh")
title = "Debunking folk tales around the town of Dirken"
body = "We've found sound waves emanating from the walls are actually coming from something we can’t see. This could be signs of ghost activity. That's what one source of a so-called ghost expert was claiming but after looking at the blue prints we can see that this sound is most likely being made by a loose bookshelf hitting the wall behind. That explains the source of the sound but how it’s so musical is still up to debate."
metadata/_custom_type_script = "uid://cvfmp86jdm3sj"

